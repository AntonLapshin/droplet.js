{
  "name": "uit",
  "version": "1.0.0",
  "description": "Building hierarchy of UI blocks",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test-0": "./node_modules/.bin/mocha --require babel-register src/**/*.test.js",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- src/**/*.js --require babel-register",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "jsdoc": "./node_modules/.bin/jsdoc src/uit.js",
    "build": "./node_modules/.bin/rollup src/uit.js --format umd --name \"uit\" --output dist/uit.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonLapshin/uit.git"
  },
  "keywords": [
    "blocks",
    "components",
    "modules",
    "observable",
    "binding"
  ],
  "author": "Anton Lapshin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AntonLapshin/uit/issues"
  },
  "homepage": "https://github.com/AntonLapshin/uit#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.3",
    "mocha": "^3.4.2",
    "rollup": "^0.43.0",
    "should": "^11.2.1"
  }
}